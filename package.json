{
  "name": "tortor",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/donatas-luciunas/tortor"
  },
  "author": "Donatas Lučiūnas",
  "license": "ISC",
  "homepage": "https://github.com/donatas-luciunas/tortor",
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [
    "onCommand:extension.tortor"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.tortor",
        "title": "Tortor"
      }
    ],
    "keybindings": [
      {
        "command": "extension.tortor",
        "key": "ctrl+shift+'",
        "mac": "cmd+shift+'",
        "when": "editorTextFocus"
      }
    ],
    "configurationDefaults": {
      "editor.tokenColorCustomizations": {
        "comments": "#FF0000",
        "textMateRules": [
          {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
              "foreground": "#FF0000"
            }
          }
        ]
      }
    },
    "scripts": {},
    "devDependencies": {
      "@types/vscode": "^1.32.0"
    },
    "dependencies": {
      "esprima": "^4.0.1",
      "ws": "^7.4.4"
    }
  }